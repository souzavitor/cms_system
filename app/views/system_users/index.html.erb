<p id="notice"><%= notice %></p>

<h1>Listing System Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Username</th>
      <th>Password</th>
      <th>St</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @system_users.each do |system_user| %>
      <tr>
        <td><%= system_user.name %></td>
        <td><%= system_user.email %></td>
        <td><%= system_user.username %></td>
        <td><%= system_user.password %></td>
        <td><%= system_user.st %></td>
        <td><%= link_to 'Show', system_user %></td>
        <td><%= link_to 'Edit', edit_system_user_path(system_user) %></td>
        <td><%= link_to 'Destroy', system_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New System user', new_system_user_path %>
